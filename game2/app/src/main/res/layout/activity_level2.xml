<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ba"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ravian.game.level2">
    <TextView
        android:id="@+id/tv1"
        android:background="#ffffffff"
        android:text="1"
        android:layout_width="90dp"
        android:layout_marginTop="10dp"
        android:layout_height="50dp"
        android:onClick="mind_Game"
        android:textAlignment="center"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/tv2"
        android:background="#ffffffff"
        android:text="2"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:onClick="mind_Game"
        android:textAlignment="center"
        android:textSize="25dp"
        android:layout_alignTop="@+id/tv1"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/tv3"
        android:background="#ffffffff"
        android:text="3"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:onClick="mind_Game"
        android:layout_alignTop="@+id/tv2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textAlignment="center"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/tv4"
        android:background="#ffffffff"
        android:text="2"
        android:layout_width="90dp"
        android:layout_marginTop="22dp"
        android:layout_height="50dp"
        android:onClick="mind_Game"
        android:layout_below="@+id/tv1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/tv5"
        android:background="#ffffffff"
        android:text="3"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:onClick="mind_Game"
        android:layout_alignTop="@+id/tv4"
        android:layout_alignLeft="@+id/tv2"
        android:layout_alignStart="@+id/tv2"
        android:textAlignment="center"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/tv6"
        android:background="#ffffffff"
        android:text="1"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:onClick="mind_Game"
        android:layout_alignTop="@+id/tv5"
        android:layout_alignLeft="@+id/tv3"
        android:layout_alignStart="@+id/tv3"
        android:textAlignment="center"
        android:textSize="22dp" />

    <Button
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:id="@+id/start_btn"
        android:onClick="mind_Game"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:background="#ffffffff"
        android:onClick="mind_Game"
        android:text="4"
        android:id="@+id/tv7"
        android:textAlignment="center"
        android:textSize="25dp"
        android:layout_marginTop="150dp"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:text="5"
        android:textAlignment="center"
        android:background="#ffffffff"
        android:onClick="mind_Game"
        android:id="@+id/tv8"
        android:textSize="25dp"
        android:layout_marginTop="150dp"

        android:layout_alignLeft="@+id/tv5" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:textAlignment="center"
        android:background="#ffffffff"
        android:text="6"
        android:onClick="mind_Game"
        android:id="@+id/tv9"
        android:layout_alignTop="@+id/tv8"
        android:layout_alignLeft="@+id/tv6"
        android:textSize="25dp" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:textAlignment="center"
        android:background="#ffffffff"
        android:text="5"
        android:id="@+id/tv10"
        android:onClick="mind_Game"
        android:layout_below="@+id/tv7"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="23dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:textAlignment="center"
        android:background="#ffffffff"
        android:text="6"
        android:onClick="mind_Game"
        android:id="@+id/tv11"
        android:layout_alignBottom="@+id/tv10"
        android:layout_alignLeft="@+id/tv5"
        android:textSize="25dp" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:textAlignment="center"
        android:background="#ffffffff"
        android:text="4"
        android:onClick="mind_Game"
        android:id="@+id/tv12"
        android:layout_alignBottom="@+id/tv11"
        android:layout_alignLeft="@+id/tv9"
        android:textSize="25dp" />

    <Button

        android:text=""
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:background="@drawable/b"
        android:id="@+id/b1"
        android:onClick="back1"
        android:layout_alignBottom="@+id/start_btn"
        android:layout_alignRight="@+id/tv10" />

</RelativeLayout>
